                            </then>
                            <else/>
                        </if>
                    </for>
                    <output expression="ap&amp;&quot; adalah bilangan prima&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="ap&amp;&quot; bukan adalah bilangan prima&quot;" newline="True"/>
                </else>
            </if>
