        <body>
            <declare name="ap, i" type="Integer" array="False" size=""/>
            <output expression="&quot;untuk menentukan bilangan prima atau bukan&quot;" newline="True"/>
            <output expression="&quot;masukkan Angka&quot;" newline="True"/>
            <input variable="ap"/>
